error:Ee=>{var Re;console.error("Error uploading file:",Ee),this.messageService.add({severity:"error",life:3e3,summary:"Error uploading file",detail:(null===(Re=Ee.error)||void 0===Re?void 0:Re.message)||"Failed to upload file"})}})}logGenerate(){if(this.editingSkill&&this.editingSkill.skillName)return this.isGenerating=!0,this.skillCategories.find(ve=>ve.category===(this.isAddingNewSkill?this.addingSkillCategoryId:this.editingState.category))?void this.jobCreationService.uploadEnhancedAPI({skill:this.editingSkill.skillName,whyItMatters:this.editingSkill.whyItMatters,significance:this.editingSkill.significanceScore,icpId:this.data._id,projectId:this.projectId}).subscribe({next:ve=>{const ne=ve.data.updated_why_it_matters;this.suggestedSkillName=ve.data.updated_skill,this.editingSkill&&(this.editingSkill.whyItMatters=ne)},error:ve=>{console.error("API call failed:",ve)},complete:()=>{this.isGenerating=!1}}):(console.error("Cannot determine category for the current skill."),void(this.isGenerating=!1));console.warn("Some fields are blank. Cannot log the payload.")}applySuggestion(){this.editingSkill&&this.suggestedSkillName&&(this.editingSkill.skillName=this.suggestedSkillName,this.suggestedSkillName=null)}}return(Fe=Ue).\u0275fac=function(ut){return new(ut||Fe)(e.Y36(t.I),e.Y36(n.v),e.Y36(y.ez))},Fe.\u0275cmp=e.Xpm({type:Fe,selectors:[["app-display-skills"]],inputs:{data:"data",roleName:"roleName",projectId:"projectId",cwProjectId:"cwProjectId",approvalJob:"approvalJob"},decls:12,vars:2,consts:[[1,"ideal-candidate-profile-step-container"],[1,"header-grid","header-row"],[1,"header-text"],[1,"header-text","center-text"],["class","skill-accordion-container",4,"ngFor","ngForOf"],["class","transfer-popup",4,"ngIf"],[1,"skill-accordion-container"],[1,"skill-accordion-header",3,"click"],[1,"skill-accordion-title"],[1,"pi","pi-angle-down","expand-toggle-btn"],["class","skill-accordion-content",4,"ngIf"],[1,"skill-accordion-content"],["class","skill-item-row",4,"ngFor","ngForO